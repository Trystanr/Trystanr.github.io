{
  "name": "Trystan Rivers New Tab",
  "description": "A custom New Tab page by Trystan Rivers",
  "version": "0.8",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage", "topSites", "identity"],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://quotesondesign.com; object-src 'self'",
  "background": {
    "scripts": ["./js/background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
